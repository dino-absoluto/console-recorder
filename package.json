{
  "name": "console-recorder",
  "version": "1.0.4",
  "description": "Record console session",
  "main": "dist/lib.js",
  "typings": "dist/console-recorder-beta.d.ts",
  "files": [
    "dist/bin.js",
    "dist/lib.js",
    "dist/console-recorder-beta.d.ts"
  ],
  "bin": {
    "console-recorder": "dist/bin.js"
  },
  "scripts": {
    "lint": "eslint --ext .js,.ts src",
    "prepublishOnly": "npm run build -- --env.prod && npm run build:extract-api",
    "build": "webpack",
    "build:extract-api": "tsc -d --emitDeclarationOnly --outDir 'built/dts/' && api-extractor run --local --verbose",
    "build:docs": "api-documenter markdown -i __tmp__/docModel -o docs/api",
    "build:etc": "npm run build:extract-api && npm run build:docs",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dino-absoluto/console-recorder.git"
  },
  "keywords": [
    "cli",
    "console",
    "record",
    "replay"
  ],
  "author": "Dino <dinoabsoluto+dev@gmail.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/dino-absoluto/console-recorder/issues"
  },
  "homepage": "https://github.com/dino-absoluto/console-recorder#readme",
  "devDependencies": {
    "@microsoft/api-documenter": "^7.1.1",
    "@microsoft/api-extractor": "^7.1.0",
    "@types/jest": "^24.0.11",
    "@types/lodash": "^4.14.123",
    "@types/node": "^11.13.5",
    "@types/webpack-bundle-analyzer": "^2.13.1",
    "@types/webpack-node-externals": "^1.6.3",
    "@types/yargs": "^13.0.0",
    "@typescript-eslint/eslint-plugin": "^1.6.0",
    "@typescript-eslint/parser": "^1.6.0",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "jest": "^24.7.1",
    "jest-extended": "^0.11.1",
    "source-map-loader": "^0.2.4",
    "ts-jest": "^24.0.2",
    "ts-loader": "^5.3.3",
    "typescript": "^3.4.3",
    "webpack": "^4.30.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.0",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "kleur": "^3.0.3",
    "lodash": "^4.17.11",
    "make-dir": "^3.0.0",
    "node-pty": "^0.8.1",
    "yargs": "^13.2.2"
  }
}
